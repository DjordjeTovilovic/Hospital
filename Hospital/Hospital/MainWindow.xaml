<Window x:Class="Hospital.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hospital"
        mc:Ignorable="d"
        Title="MainWindow" Height="750" Width="430">
    <Grid Background="#81c1b0">
        <Button HorizontalAlignment="Left" Height="66" Margin="243,320,0,0" VerticalAlignment="Top" Click="ViewRooms_Click" Width="70">
            <Image Stretch="Fill" Source="/Images/room.png" Height="64" Width="66"/>
        </Button>
        <Button HorizontalAlignment="Left" Height="66" Margin="108,406,0,0" VerticalAlignment="Top" Width="70"  Click="ViewDynamic_Click">
            <Image Stretch="Fill" Source="/Images/dynamic.png" Height="64" Width="66"/>
        </Button>
        <Button HorizontalAlignment="Left" Height="66" Margin="109,320,0,0" VerticalAlignment="Top" Width="70" Click="ViewMedicine_Click">
            <Image Stretch="Fill" Source="/Images/medicine.png" Height="64" Width="66"/>
        </Button>
        <Button HorizontalAlignment="Left" Height="66" Margin="243,406,0,0" VerticalAlignment="Top" Width="70"  Click="ViewStatic_Click">
            <Image Stretch="Fill" Source="/Images/bed.png" Height="64" Width="66"/>
        </Button>

    </Grid>
</Window>
